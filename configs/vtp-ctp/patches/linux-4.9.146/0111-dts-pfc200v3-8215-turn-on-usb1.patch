From 0fdf2428505d3095ef79319d00d6bef194a674a0 Mon Sep 17 00:00:00 2001
From: Heinrich Toews <heinrich.toews@wago.com>
Date: Tue, 25 Jul 2017 12:06:36 +0000
Subject: [PATCH] dts: pfc200v3: 8215: turn on usb1

---
 arch/arm/boot/dts/wago-pfc200v3.dtsi | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/arm/boot/dts/wago-pfc200v3.dtsi b/arch/arm/boot/dts/wago-pfc200v3.dtsi
index 8fdf2a6..01641f7 100644
--- a/arch/arm/boot/dts/wago-pfc200v3.dtsi
+++ b/arch/arm/boot/dts/wago-pfc200v3.dtsi
@@ -850,19 +850,19 @@
 };
 
 &usb {
-	status = "disabled";
+	status = "okay";
 };
 
 &usb_ctrl_mod {
-	status = "disabled";
+	status = "okay";
 };
 
 &usb1_phy {
-	status = "disabled";
+	status = "okay";
 };
 
 &usb1 { /* usb1 - OTG */
-	status = "disabled";
+	status = "okay";
 	dr_mode = "host";
 };
 
